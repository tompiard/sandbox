---
const { tag } = Astro.props
const apiKey = 'eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzUxMiIsImtpZCI6IjI4YTMxOGY3LTAwMDAtYTFlYi03ZmExLTJjNzQzM2M2Y2NhNSJ9..8r8bdkT7KbSbwj-jPX7NoFgyLrMMM9ZcoYHeGZgY9hEmays1lqZy5qxRlwfeuVuR_YRHBXRdvThpXv0yB7Wrhg'
const data = await fetch(
    'https://api.clashroyale.com/v1/players/' + encodeURI(tag),
    {
        headers: new Headers({
            'Authorization': 'Authorization: Bearer ' + apiKey
        }), 
    }
).then(r => r.json())
---
<ul>
    {data.map((item) => (
      <li>{item}</li>
    ))}
</ul>
